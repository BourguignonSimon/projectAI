# Silent Factory Environment Configuration
# Copy this file to .env and fill in your values

# Google Gemini API Key (required)
# Get your key at: https://aistudio.google.com/apikey
GOOGLE_API_KEY="your-api-key-here"

# Redis Configuration
# Main Redis instance for agent communication
REDIS_HOST="localhost"
REDIS_PORT=6380

# Routing Redis instance for orchestrator routing decisions
REDIS_ROUTING_PORT=6381

# AI Model Configuration
# MODEL_SMART: Used for complex tasks (Manager, Analyst, Architect)
# MODEL_FAST: Used for quick tasks (Coder, Reviewer)
MODEL_SMART="gemini-2.5-pro"
MODEL_FAST="gemini-2.5-flash"
